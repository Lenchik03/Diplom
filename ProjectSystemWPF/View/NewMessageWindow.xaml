        xmlns:local="clr-namespace:ProjectSystemWPF"
        xmlns:vm="clr-namespace:ProjectSystemWPF.ViewModel"
        mc:Ignorable="d"
        Title="NewMessageWindow" Height="450" Width="800"
        DataContext="{DynamicResource messageVM}">

    <Window.Resources>
        <vm:MessageVM x:Key="messageVM"/>
    </Window.Resources>
    <Grid>
        <StackPanel x:Name="stackpanel">
           
        </StackPanel>

        
    </Grid>
</Window>
